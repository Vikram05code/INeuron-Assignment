package com.vikram;

public class INeuronPattern {
	public static void main(String[] args) {
		/*
		 * Assignment 1 
		 * 1. Write a program to print INEURON using pattern programming
		 * logic.
		 */
		
		int n = 10;
		for(int i=0; i<n; i++) {
			for(int j=0; j<n; j++) {
				if(j==n/2) {
					System.out.print("*");
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println();
			
		}
	}

}
